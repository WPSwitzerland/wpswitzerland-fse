{
	"settings": {
		"appearanceTools": false,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"background": true,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"gradients": [
				{
					"gradient": "linear-gradient(180deg,#00334D 0%,#0073aa 100%)",
					"name": "WordPress blue",
					"slug": "wordpress-blue"
				}
			],
			"link": true,
			"palette": [
				{
					"color": "#0073aa",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#00334D",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#4d4d4d",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#F0F0F0",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			],
			"text": true
		},
		"custom": {
			"body": {
				"typography": {
					"lineHeight": 1.5
				}
			},
			"button": {
				"spacing": {
					"padding": {
						"bottom": ".4em",
						"top": ".4em"
					}
				}
			},
			"fontSizes": {
				"step-l": "clamp(1.27rem, calc(1.19rem + 0.38vw), 1.8rem)",
				"step-m": "clamp(1.13rem, calc(1.07rem + 0.27vw), 1.5rem)",
				"step-r": "clamp(1rem, calc(0.96rem + 0.18vw), 1.0625rem)",
				"step-s": "clamp(0.89rem, calc(0.87rem + 0.11vw), 1.04rem)",
				"step-xl": "clamp(1.42rem, calc(1.32rem + 0.53vw), 2.16rem)",
				"step-xs": "clamp(0.79rem, calc(0.78rem + 0.06vw), 0.87rem)",
				"step-xxl": "clamp(1.6rem, calc(1.46rem + 0.71vw), 2.59rem)",
				"step-xxxl": "clamp(1.8rem, calc(1.61rem + 0.93vw), 3.11rem)",
				"step-xxxxl": "clamp(2.03rem, calc(1.78rem + 1.22vw), 3.73rem)"
			},
			"gallery": {
				"caption": {
					"fontSize": "var(--wp--custom--font-sizes--step-l)"
				}
			},
			"image": {
				"caption": {
					"fontSize": "var(--wp--custom--font-sizes--step-l)"
				}
			},
			"spacing-outer-gutter": "var(--unit)",
			"spacing-unit": "1rem"
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1280px"
		},
		"spacing": {
			"blockGap": true,
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Archivo",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Archivo-VariableFont_wdth,wght.woff2"
							]
						},
						{
							"fontFamily": "Archivo",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Archivo-VariableFont_wdth,wght.woff2"
							]
						},
						{
							"fontFamily": "Archivo",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Archivo-VariableFont_wdth,wght.woff2"
							]
						},
						{
							"fontFamily": "Archivo",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Archivo-VariableFont_wdth,wght.woff2"
							]
						}
					],
					"fontFamily": "Archivo, sans-serif",
					"name": "Archivo",
					"slug": "primary"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Gilroy",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/Gilroy-ExtraBold.woff2"
							]
						}
					],
					"fontFamily": "Gilroy, sans-serif",
					"name": "Gilroy",
					"slug": "secondary"
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"name": "X Small",
					"size": "var(--wp--custom--font-sizes--step-xs)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "var(--wp--custom--font-sizes--step-s)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "var(--wp--custom--font-sizes--step-m)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "var(--wp--custom--font-sizes--step-l)",
					"slug": "large"
				},
				{
					"name": "X Large",
					"size": "var(--wp--custom--font-sizes--step-xl)",
					"slug": "x-large"
				},
				{
					"name": "XX Large",
					"size": "var(--wp--custom--font-sizes--step-xxl)",
					"slug": "xx-large"
				},
				{
					"name": "XXX Large",
					"size": "var(--wp--custom--font-sizes--step-xxxl)",
					"slug": "xxx-large"
				},
				{
					"name": "XXXX Large",
					"size": "var(--wp--custom--font-sizes--step-xxxxl)",
					"slug": "xxxx-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"typography": {
					"fontWeight": "600"
				}
			},
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontWeight": "900"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontSize": "var(--wp--custom--font-sizes--step-r)",
					"fontWeight": "700"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-xxxxl)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-xxxl)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-xxl)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-xl)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-l)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-m)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontWeight": "600"
				}
			}
		},
		"spacing": {
			"blockGap": "2vw"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--custom--font-sizes--step-r)",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}