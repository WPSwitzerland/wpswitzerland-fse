{
	"settings": {
		"appearanceTools": true,
		"border": {
			"color": false,
			"radius": false,
			"style": false,
			"width": false
		},
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"duotone": [
				{
					"colors": ["#00334D", "#0073aa"],
					"name": "WordPress blue",
					"slug": "wordpress-blue"
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg,#00334D 0%,#0073aa 100%)",
					"name": "WordPress blue",
					"slug": "wordpress-blue"
				}
			],
			"palette": [
				{
					"color": "#0073aa",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#004566",
					"name": "Primary dark",
					"slug": "primary-dark"
				},
				{
					"color": "#00334D",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#4d4d4d",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#F0F0F0",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#1d2327",
					"name": "Darkest gray",
					"slug": "darkest-gray"
				},
				{
					"color": "#b5007c",
					"name": "Geneva 2021",
					"slug": "custom-geneva-2021"
				},
				{
					"color": "#cc8866",
					"name": "Murten 2023",
					"slug": "custom-murten-2023"
				},
				{
					"color": "#f55050",
					"name": "WCEU 2025",
					"slug": "custom-wceu-2025"
				}
			],
			"text": true
		},
		"custom": {
			"body": {
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--body)"
				}
			},
			"button": {
				"spacing": {
					"padding": {
						"bottom": ".4em",
						"top": ".4em"
					}
				}
			},
			"fontSizes": {
				"step-xs": "clamp(0.7901rem, 0.7605rem + 0.1481vi, 0.9383rem)",
				"step-s": "clamp(0.8889rem, 0.8556rem + 0.1667vi, 1.0556rem)",
				"step-r": "clamp(1rem, 0.9625rem + 0.1875vi, 1.1875rem)",
				"step-m": "clamp(1.125rem, 1.0828rem + 0.2109vi, 1.3359rem)",
				"step-l": "clamp(1.2656rem, 1.2182rem + 0.2373vi, 1.5029rem)",
				"step-xl": "clamp(1.4238rem, 1.3704rem + 0.267vi, 1.6908rem)",
				"step-xxl": "clamp(1.6018rem, 1.5417rem + 0.3003vi, 1.9021rem)",
				"step-xxxl": "clamp(1.802rem, 1.6894rem + 0.5631vi, 2.3652rem)",
				"step-xxxxl": "clamp(2.0273rem, 1.9513rem + 0.3801vi, 2.4074rem)"
			},
			"lineHeight": {
				"body": "1.5",
				"heading": "1.2"
			},
			"spacing-outer-gutter": "var(--unit)",
			"spacing-unit": "1rem"
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1280px"
		},
		"spacing": {
			"spacingScale": {
				"increment": 1.5,
				"mediumStep": 1.5,
				"operator": "*",
				"steps": 6,
				"unit": "rem"
			},
			"units": ["px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Archivo",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/Archivo-VariableFont_wdth,wght.woff2"]
						},
						{
							"fontFamily": "Archivo",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/Archivo-VariableFont_wdth,wght.woff2"]
						},
						{
							"fontFamily": "Archivo",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/Archivo-VariableFont_wdth,wght.woff2"]
						},
						{
							"fontFamily": "Archivo",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/Archivo-VariableFont_wdth,wght.woff2"]
						}
					],
					"fontFamily": "Archivo, sans-serif",
					"name": "Archivo",
					"slug": "primary"
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"name": "X Small",
					"size": "var(--wp--custom--font-sizes--step-xs)",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "var(--wp--custom--font-sizes--step-s)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "var(--wp--custom--font-sizes--step-m)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "var(--wp--custom--font-sizes--step-l)",
					"slug": "large"
				},
				{
					"name": "X Large",
					"size": "var(--wp--custom--font-sizes--step-xl)",
					"slug": "x-large"
				},
				{
					"name": "XX Large",
					"size": "var(--wp--custom--font-sizes--step-xxl)",
					"slug": "xx-large"
				},
				{
					"name": "XXX Large",
					"size": "var(--wp--custom--font-sizes--step-xxxl)",
					"slug": "xxx-large"
				},
				{
					"name": "XXXX Large",
					"size": "var(--wp--custom--font-sizes--step-xxxxl)",
					"slug": "xxxx-large"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "2px"
				},
				"color": {
					"background": "var:preset|color|primary",
					"text": "var:preset|color|white"
				},
				"typography": {
					"fontWeight": "600"
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": {
						"left": "var:preset|spacing|50",
						"top": "var:preset|spacing|50"
					}
				}
			},
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
					"fontWeight": "900",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"core/post-title": {
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--primary)",
					"fontWeight": "700"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-xxxxl)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-xxxl)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-xxl)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-xl)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-l)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--custom--font-sizes--step-m)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontWeight": "600"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--custom--font-sizes--step-r)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "mobilemenu",
			"title": "Mobile menu"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"customTemplates": [
		{
			"name": "without_header",
			"postTypes": ["page"],
			"title": "Without cover block header"
		}
	]
}
