{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "sht/menu",
	"title": "Navigation",
	"category": "theme",
	"icon": "menu",
	"description": "",
	"textdomain": "sha",
	"keywords": ["menu", "navigation"],
	"attributes": {
		"blockId": {
			"type": "string"
		},
		"menu": {
			"type": "string",
			"default": "primary"
		}
	},
	"supports": {
		"align": ["wide", "full", "left", "right", "center"],
		"color": {
			"text": true,
			"link": true,
			"background": false,
			"gradients": false
		},
		"typography": { "fontSize": true, "fontWeight": true },
		"html": false,
		"padding": false,
		"margin": false
	},
	"editorStyle": "file:./assets/dist/styles/editor/index.min.css",
	"editorScript": "file:./assets/dist/scripts/editor.js",
	"style": "file:./assets/dist/styles/view/index.min.css",
	"render": "file:./render.php"
}
